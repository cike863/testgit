package com.xsbweb.vo;

import java.util.List;

public class ProjectAndItems {
	private TrsProject trsProject;
	private List<ProjectItem> projectItems;
	public TrsProject getTrsProject() {
		return trsProject;
	}
	public void setTrsProject(TrsProject trsProject) {
		this.trsProject = trsProject;
	}
	public List<ProjectItem> getProjectItems() {
		return projectItems;
	}
	public void setProjectItems(List<ProjectItem> projectItems) {
		this.projectItems = projectItems;
	}
}
